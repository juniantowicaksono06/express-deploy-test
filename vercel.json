{
	"version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install --legacy-peer-deps",
	"builds": [
		{
			"src": "dist/index.js",
			"use": "@vercel/node",
			"config": { "includeFiles": ["dist/**"] }
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.js"
		}
	]
}